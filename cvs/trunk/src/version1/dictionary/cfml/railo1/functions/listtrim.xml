<?xml version="1.0" encoding="UTF-8"?>

<!--
	String ListTrim(list [, delimiters ])
-->
<function xmlns="http://www.cfeclipse.org/version1/dictionary"
	name="listtrim" returns="Numeric">
	<help><![CDATA[
		Trims a list by removing empty values at 
		the beginning and the end of the list.
		Example:
		listTrim(',,,,a,b,c,,,,')
		Returns 'a,b,c'
	]]></help>
	<parameter name="list" type="String" required="true">
		<help><![CDATA[
		]]></help>
	</parameter>
	<parameter name="delimiters" type="String" required="false">
		<help><![CDATA[
		]]></help>
		<values default=",">
			<value option="," />
			<value option="|" />
			<value option=";" />
			<value option="chr(9)" />
			<value option="chr(10)" />
			<value option="chr(13)" />
		</values>
	</parameter>
</function>
