<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.tigris.cfeclipse.CFE_MachII"
   name="CFEclipse Mach-II Framework Plug-in"
   version="0.0.1"
   provider-name="The CFE Mach-II Framework Team"
   class="org.tigris.cfeclipse.machII.CFE_MachIIPlugin">

   <runtime>
      <library name="CFE_MachII.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="com.rohanclan.cfml"/>
   </requires>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.tigris.cfeclipse.machII.views.MachIIView"
            name="Mach II Explorer"
            id="org.tigris.cfeclipse.machii.views.MachIIView"/>
   </extension>
   <extension
         point="com.rohanclan.cfml.cacorNatures">
      <CACorContributingNature
            label="Mach-II"
            class="org.tigris.cfeclipse.machII.nature.MachIINature"
            id="org.tigris.cfeclipse.machII.nature.MachIINature"/>
   </extension>

</plugin>
